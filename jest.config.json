{
    "collectCoverageFrom": [
        "**/*.{js,jsx}",
        "!**/node_modules/**",
        "!**/vendor/**",
        "!./coverage/**"
      ],
      "clearMocks": true,
      "resetMocks": true,
      "resetModules": true,
       "coverageThreshold": {
        "global": {
            "branches": 56,
            "functions": 75,
            "lines": 66,
            "statements": 65
        }
    }
}